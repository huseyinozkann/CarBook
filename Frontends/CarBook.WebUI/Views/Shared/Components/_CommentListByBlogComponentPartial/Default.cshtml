@model List<ResultCommentDto>

<ul class="comment-list">
    @foreach (var item in Model)
    {
        <li class="comment">
           
            <div class="comment-body">
                <h3>@item.Name</h3>
                <div class="meta">@item.CreatedDate.ToShortDateString()</div>
                <p>@item.Description</p>
            </div>
        </li>
    }


</ul>

